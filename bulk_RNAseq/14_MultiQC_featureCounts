# MultiQC to compile STAR log output into 1 report: https://multiqc.info/
# Run locally (MultiQC version 1.12)

multiqc ../../RAW_COUNTS/HOWELL_GRCh37 -n 09_GRCh37AlignedBAMs_MultiQCReport-Howell.html --interactive --zip-data-dir &> 09_GRCh37AlignedBAMs_MultiQC-Howell.log
multiqc ../../RAW_COUNTS/HOWELL_GRCh38 -n 09_GRCh38AlignedBAMs_MultiQCReport-Howell.html --interactive --zip-data-dir &> 09_GRCh38AlignedBAMs_MultiQC-Howell.log

multiqc ../../RAW_COUNTS/RE_GRCh37_per-gene-counts.txt.summary -n 14_GRCh37featureCounts_MultiQCReport-40706.html --interactive --zip-data-dir &> 14_GRCh37featureCounts_MultiQC-40706.log
multiqc ../../RAW_COUNTS/RE_GRCh38_per-gene-counts.txt.summary -n 14_GRCh38featureCounts_MultiQCReport-40706.html --interactive --zip-data-dir &> 14_GRCh38featureCounts_MultiQC-40706.log

multiqc ../../RAW_COUNTS/AF_GRCh37_per-gene-counts.txt.summary -n 14_GRCh37featureCounts_MultiQCReport-40750.html --interactive --zip-data-dir &> 14_GRCh37featureCounts_MultiQC-40750.log
multiqc ../../RAW_COUNTS/AF_GRCh38_per-gene-counts.txt.summary -n 14_GRCh38featureCounts_MultiQCReport-40750.html --interactive --zip-data-dir &> 14_GRCh38featureCounts_MultiQC-40750.log
